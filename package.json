    "boilerplate",
    "seed",
    "app",
    "typescript",
    "sass",
    "mobile",
    "web"
  ],
  "author": "Yohan Lasorsa",
  "license": "MIT",
  "preferGlobal": true,
  "main": "./generators/app/index.js",
