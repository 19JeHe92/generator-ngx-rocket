{
  "name": "ng2-starter-kit",
  "version": "0.9.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build --aot --prod",
    "start": "ng serve --proxy-config proxy.conf.js",
    "lint": "tslint \"src/**/*.ts\" && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\"",
    "test": "ng test",
    "test:ci": "ng test --single-run --code-coverage --lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "translations:extract": "ng2-translate-extract --dir ./src --output ./src/translations --format=pot --clean",
    "docs": "hads ./docs -o"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@biesbjerg/ng2-translate-po-loader": "^0.2.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.18",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "ng2-translate": "^5.0.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@biesbjerg/ng2-translate-extract": "^0.5.1",
    "@types/jasmine": "^2.5.40",
    "@types/lodash": "^4.14.48",
    "@types/node": "^7.0.0",
    "angular-cli": "1.0.0-beta.25.5",
    "codelyzer": "~2.0.0-beta.1",
    "hads": "^1.3.1",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^1.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "~1.4.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~4.0.13",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.10"
  }
}
